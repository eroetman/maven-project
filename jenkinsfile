pipeline {

    agent any

    stages{
        stage('Build'){
            steps {
                Artifactory.newMavenBuild().run pom: 'pom.xml', goals: 'clean package'
                // sh 'mvn clean package'
            }
            post {
                success {
                    echo 'Now Archiving...'
                    archiveArtifacts artifacts: '**/target/*.war'
                }
            }
        }
    }
}